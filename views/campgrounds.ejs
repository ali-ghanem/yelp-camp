<%- include("partials/header") %> 

<h1>Campgrounds</h1>

<a href="/campgrounds/new">Add a new campground</a>

<% campgrounds.forEach(campground => { %>
    <div>
        <h3><%= campground.name %> </h3>
        <img src="<%= campground.image %>" alt="">
    </div>
<% }) %>

